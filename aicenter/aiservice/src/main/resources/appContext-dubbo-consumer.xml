<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://wwww.w3.org/2001/XMLSchema-instance" xmlns:dubbo="http://www.alibabatech.com/schema/dubbo"
	xsi:schemaLocation="http://www.springframework.org/schema/beans 
	http://www.springframework.org/schema/beans/spring-beans.xsd 
	http://code.alibabatech.com/schema/dubbo 
	http://code.alibabatech.com/schema/dubbo/dubbo.xsd">

	<dubbo:application name="aicenter" />
	<dubbo:registry address="zookeeper://127.0.0.1:2191" />
	<dubbo:protocol name="dubbo" port="20001" />
	<dubbo:provider loadbalance="random" />

	<dubbo:reference id="picSearchFacade"
		class="net.picenter.picfacade.PicSearchFacade" />

</beans>